
{
  "name": "dslink-javascript-salesforce",
  "version": "0.0.1",
  "description": "DSLink for Salesforce SOQL and CRUD functions",
  "author": {
    "name": "Kerry Gould",
    "email": "k.gould@dglogik.com"
  },
  "main": "salesforce.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "getDependencies": [
    "npm install"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-javascript-salesforce"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-javascript-salesforce/issues"
  },
  "configs": {
    "broker": {
      "type": "url",
      "required": false
    },
    "nodes": {
      "type": "path",
      "value": "nodes.json",
      "required": true
    },
    "key": {
      "type": "path",
      "value": ".dslink.key",
      "required": true
    }
  },
  "license": "Apache"
}